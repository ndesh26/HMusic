
<!-- Static navbar -->
<nav .navbar.navbar-default.navbar-static-top>
    <div .container>
        <div .navbar-header>
            <button type="button" .navbar-toggle.collapsed data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>

        <div #navbar .collapse.navbar-collapse>
            <ul .nav.navbar-nav>
                $forall MenuItem label route _ <- navbarLeftFilteredMenuItems
                    <li :Just route == mcurrentRoute:.active>
                        <a href="@{route}">#{label}

            <ul .nav.navbar-nav.navbar-right>
                $forall MenuItem label route _ <- navbarRightFilteredMenuItems
                    <li :Just route == mcurrentRoute:.active>
                        <a href="@{route}">#{label}

<!-- Page Contents -->

<div .container>
    $if not $ Just HomeR == mcurrentRoute
        <ul .breadcrumb>
            $forall bc <- parents
                <li>
                    <a href="@{fst bc}">#{snd bc}

            <li .active>#{title}

    $maybe msg <- mmsg
      <div .alert.alert-info #message>#{msg}


$if (Just HomeR == mcurrentRoute)
    ^{widget}
$else
    <div .container>
        <div .row>
            <div .col-md-12>
                ^{widget}

<!-- Footer -->
<footer .footer>
    <div #player>
        <div id="title">
            <span id="track">
            <div id="timer">
                0:00
            <div id="duration">
                0:00
        
          <!-- Controls -->
        <div class="controlsOuter">
            <div class="controlsInner">
              <div id="loading">
              <img class="btn-player" id="playBtn" src="data:image/png;base64,">
              <img class="btn-player" id="pauseBtn" src="data:image/png;base64,">
              <img class="btn-player" id="prevBtn">
              <img class="btn-player" id="nextBtn">
            
            <div class="btn-player" id="playlistBtn">
            <div class="btn-player" id="volumeBtn">
          
        
          <!-- Progress -->
        <div id="waveform">
        <div id="bar">
        <div id="progress">
        
          <!-- Playlist -->
        <div id="playlist">
            <div id="list">
          
        
          <!-- Volume -->
        <div id="volume" class="fadeout">
            <div id="barFull" class="bar">
            <div id="barEmpty" class="bar">
            <div id="sliderBtn">


<!-- Scripts -->
<script src="../static/js/howler.core.js">
<script src="../static/js/siriwave.js">
<script src="../static/js/player.js">
<link rel="stylesheet" href="../static/css/styles.css">
